{
  "name": "rapidbundle",
  "version": "0.2.2",
  "repository": "git@github.com:umidbekk/rapidbundle.git",
  "license": "MIT",
  "author": "Umidbek Karimov <4734297+umidbekk@users.noreply.github.com>",
  "bin": {
    "rapidbundle": "bin"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "jest --coverage",
    "lint": "eslint .",
    "format": "prettier . --write",
    "tdd": "jest --watch",
    "release": "np --no-2fa"
  },
  "lint-staged": {
    ".js": [
      "eslint --fix",
      "prettier --write",
      "jest --bail --findRelatedTests"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2019
    },
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended-script"
    ],
    "overrides": [
      {
        "files": "*.spec.js",
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "examples",
    "test/__fixtures__"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/",
      "<rootDir>/node_modules/",
      "<rootDir>/test/__fixtures__/"
    ]
  },
  "dependencies": {
    "debug": "^4.3.1",
    "esbuild": "^0.11.20",
    "find-up": "^5.0.0",
    "listr": "^0.14.3",
    "semver": "^7.3.5",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/listr": "^0.14.3",
    "@types/lodash": "^4.14.168",
    "@types/semver": "^7.3.5",
    "eslint": "^7.26.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "prettier-plugin-jsdoc": "^0.3.22",
    "prettier-plugin-organize-imports": "^2.0.0",
    "prettier-plugin-packagejson": "^2.2.11",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=12"
  }
}
